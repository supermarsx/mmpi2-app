{
  "name": "mmpi2",
  "version": "0.0.1",
  "description": "MMPI2 test",
  "main": "index.js",
  "scripts": {
    "start": "npx live-server dist/",
    "ts-build": "tsc --build",
    "ts-build-clean": "tsc --build --clean && tsc --build",
    "less-compile": "npx lessc ./source/less/app.less ./dist/css/app.css",
    "test": "echo \"Error: no test specified\" && exit 1",
    "copy-html": "xcopy .\\source\\html\\* .\\dist\\ /E /S /V /D /Y",
    "buildAndCopy": "npm run copy-html && npm run ts-build",
    "watch-dev": "npx watch \"npm run buildAndCopy\" ./source",
    "watch-dev-alt": "npx watch \"npm run copy-html && npx lessc ./source/less/app.less ./dist/css/app.css\" ./source",
    "build-server": "npx esbuild .\\source\\server.ts --outfile=.\\server.js",
    "watch-server-ts": "npx watch \"npm run build-server\" ./source",
    "watch-server-dev": "npx nodemon ./source/server.ts"
  },
  "type": "commonjs",
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jquery": "^3.5.16",
    "http-server": "^14.1.1",
    "kill-port": "^2.0.1",
    "less": "^4.1.3",
    "live-server": "^1.2.2",
    "nodemon": "^3.0.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "esbuild": "0.19.3",
    "express": "^4.18.2",
    "nodemailer": "^6.9.5"
  }
}
